        <arguments>
          <argument>
            <kind>1</kind>
            <string>if round(global.render) &lt;= round(global.maximum)
    {
    scr_dex()
    //abort = scr_battleforms()
    abort = false
    scr_forms_skip()
    own = global.pokemon[idno]
    
    //Jumping
    if x &gt; round((global.width*6)*global.mult)
        {
        x = round((global.width/2)*global.mult)
        y += round((global.width)*global.mult)
        }
    if (global.boxcount = 30 &amp;&amp; !(dexno = 152 || dexno = 252 || dexno = 387 || dexno = 494 || dexno = 650 || dexno = 722))
        {
        y += ((global.width)*global.mult)/2
        global.boxcount = 0
        }
    if (dexno = 152 || dexno = 252 || dexno = 387 || dexno = 494 || dexno = 650 || dexno = 722)
        {
        if x != (global.width/2)*global.mult
            y += ((global.width)*global.mult)
        x = (global.width/2)*global.mult
        y += ((global.width)*global.mult)/2
        global.boxcount = 0
        }

    if (dexno = 493 || dexno = 649 || dexno = 710 || dexno = 711 || dexno = 773)
        {
        sprite = scr_spr(dexno)
        }
    if dexno = 201
        {
        if form = "!"
            sprite = scr_spr(string(dexno)+"_EM")
        if form = "?"
            sprite = scr_spr(string(dexno)+"_QM")
        }
    
    //Male/Female
    //if (sprite = -1 &amp;&amp; (form = "MALE" || form = "FEMALE"))
    if !file_exists("icons\"+string(dexno)+string(form)+".png")
        sprite = sprite_add("icons\"+string(dexno)+".png",0,true,false,0,0);
    
    global.render++
    global.boxcount++
    alarm[0] = 2//30//!V2 1
    alarm[1] = room_speed/4
    outline = false    
    lock = false
    lock_index = 0
    }
else
    {
    //room_speed = 60
    with self instance_destroy()
    }
    
    
</string>
          </argument>
        </arguments>
      </action>
    </event>
