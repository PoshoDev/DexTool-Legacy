if own = true
    if outline = true
        draw_set_color(c_maroon)
    else
        draw_set_color(c_red)
else
    if outline = true
        draw_set_color(c_gray)
    else
        draw_set_color(c_white)
    
draw_rectangle(x,y,x+(global.width*global.mult)-1,y+(global.width*global.mult)-1,false)

draw_sprite_ext(spr_slot,0,x,y,global.mult,global.mult,image_angle,image_blend,image_alpha)

if sprite != -1
    {
    draw_sprite_ext(sprite,0,x,y+(5*global.mult),global.mult,global.mult,image_angle,image_blend,image_alpha)
    }

//Female/Male/Mega tags    
switch(form)
{
    case "FEMALE":
        draw_sprite_ext(spr_tag,0,x,y,global.mult,global.mult,image_angle,image_blend,image_alpha)
    break;
    
    case "MALE":
        draw_sprite_ext(spr_tag,1,x,y,global.mult,global.mult,image_angle,image_blend,image_alpha)
    break;
    
    case "MEGA":
        draw_sprite_ext(spr_tag,2,x,y,global.mult,global.mult,image_angle,image_blend,image_alpha)
    break;
}

//Arceus &amp; Silvally tags
